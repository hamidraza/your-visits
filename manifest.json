{
    "name": "YourVisits",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Track your website visits",
    "icons": {
        "16": "img/icon-16.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["content.js"],
        "css": ["content.css"]
    }]
}